{
  "name": "lacona-settings",
  "version": "1.0.0",
  "description": "Lacona Command for modifying system settings",
  "lacona": {
    "title": "System Settings",
    "customPreferences": true,
    "description": "Set various system settings on-the-fly."
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-settings.git"
  },
  "keywords": [
    "lacona",
    "command",
    "settings"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-settings/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-settings",
  "dependencies": {
    "elliptical": "^1.0.0",
    "lacona-api": "^1.0.0",
    "lacona-phrases": "^1.0.0",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "rimraf": "^2.5.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-class-properties"
    ]
  }
}
